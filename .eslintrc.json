{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "type",
          "index",
          "object"
        ]
      }
    ]
  }
}
